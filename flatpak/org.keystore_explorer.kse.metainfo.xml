<?xml version='1.0' encoding='utf-8'?>
<component type="desktop">
  <id>org.keystore-explorer.kse</id>
  <name>KeyStore Explorer</name>
  <summary>KeyStore Explorer presents Java keystore files, and more, via an intuitive graphical user interface.</summary>
  <developer_name>Wayne Grant, Kai Kramer</developer_name>
  <launchable type="desktop-id">org.keystore_explorer.kse.desktop</launchable>
  <metadata_license>CC0-1.0</metadata_license>
  <project_license>GPL-3.0</project_license>
  <description>
    <p>KeyStore Explorer is an open source GUI replacement for the Java command-line utilities keytool and jarsigner. KeyStore Explorer presents their functionality, and more, via an intuitive graphical user interface.</p>
  </description>
  <screenshots>
    <screenshot type="default">
      <image type="source">https://raw.githubusercontent.com/kaikramer/kaikramer.github.io/main/images/win10_mykeystore.png</image>
    </screenshot>
  </screenshots>
  <releases>
    <release version="v5.5.3" date="2023-12-29" type="stable">
      <url>https://github.com/kaikramer/keystore-explorer/releases/tag/v5.5.3</url>
    </release>
  </releases>
  <url type="homepage">https://keystore-explorer.org/index.html</url>
  <url type="bugtracker">https://github.com/kaikramer/keystore-explorer/issues</url>
  <categories>
    <category>Utility</category>
    <category>Certificates</category>
  </categories>
  <recommends>
    <control>pointing</control>
    <control>keyboard</control>
  </recommends>
  <content_rating type="oars-1.1"/>
  <provides>
    <mediatype>application/x-java-archive</mediatype>
    <mediatype>application/java-archive</mediatype>
    <mediatype>application/x-jar</mediatype>
  </provides>
  <keywords>
    <keyword>Certificate</keyword>
    <keyword>Cryptography</keyword>
    <keyword>Keystore</keyword>
    <keyword>Java</keyword>
  </keywords>
</component>
